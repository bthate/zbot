import os, sys

#skip = "E0202,W0703,R0914,C0415,W0511,W0201,R0902,R0912,R0915,W0221,W0212,W0622,R0801,R0903,W0612,C0410,C0411,C0116,C0103,C0114,C0115,C0301,W0613,E0102,E1101,R0201,R1710"
