#!/usr/bin/python3 -u
# ZBOT - 24/7 channel daemon.
#
#

import os
import sys

sys.path.insert(0, os.getcwd())

from zbot.obj import cdir, os, sys
from zbot.csl import Console, boot, execute

import zbot

def main():
    k = boot("zcmd")
    #k.walk("zbot", "irc")
    return k.cmd(k.cfg.origtxt)

execute(main)
os._exit(0)
